{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "Debug",
      "type": "go",
      "request": "launch",
      "mode": "debug",
      "program": "${workspaceFolder}/main.go",
      "buildFlags": "-tags=debug",
      "args": [
        "wal-restore",
        "--http-server-addr",
        ":8080",
        "--http-server-token",
        "1024",
        "--log-level",
        "debug",
        "0000000100000005000000F6",
        "."
      ],
      "env": {
        "PGHOST": "localhost",
        "PGPORT": "5432",
        "PGUSER": "postgres",
        "PGPASSWORD": "postgres",
      },
      "console": "integratedTerminal"
    }
  ]
}