{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "Debug",
      "type": "go",
      "request": "launch",
      "mode": "debug",
      "program": "${workspaceFolder}/main.go",
      "buildFlags": "-tags=debug",
      "args": [
        "-D",
        "wals",
        "-S",
        "pg_recval_5",
        "--log-level",
        "info"
      ],
      "env": {
        "PGHOST": "localhost",
        "PGPORT": "5432",
        "PGUSER": "postgres",
        "PGPASSWORD": "postgres",
      },
      "console": "integratedTerminal"
    }
  ]
}