{
  "uid": "pgrwl-basic",
  "title": "PGRWL: WAL Upload & Retention (Basic Counters)",
  "tags": [
    "pgrwl",
    "postgres",
    "wal",
    "upload",
    "basic"
  ],
  "timezone": "browser",
  "schemaVersion": 30,
  "version": 1,
  "panels": [
    {
      "type": "stat",
      "title": "WAL Bytes Received",
      "gridPos": {
        "x": 0,
        "y": 0,
        "w": 12,
        "h": 6
      },
      "targets": [
        {
          "expr": "pgrwl_wal_bytes_received_total",
          "format": "time_series"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "bytes"
        }
      }
    },
    {
      "type": "stat",
      "title": "WAL Files Received",
      "gridPos": {
        "x": 0,
        "y": 6,
        "w": 12,
        "h": 6
      },
      "targets": [
        {
          "expr": "pgrwl_wal_files_received_total",
          "format": "time_series"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "none"
        }
      }
    },
    {
      "type": "stat",
      "title": "WAL Files Uploaded",
      "gridPos": {
        "x": 0,
        "y": 12,
        "w": 12,
        "h": 6
      },
      "targets": [
        {
          "expr": "pgrwl_wal_files_uploaded_total",
          "format": "time_series"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "none"
        }
      }
    },
    {
      "type": "stat",
      "title": "WAL Files Deleted",
      "gridPos": {
        "x": 0,
        "y": 18,
        "w": 12,
        "h": 6
      },
      "targets": [
        {
          "expr": "pgrwl_wal_files_deleted_total",
          "format": "time_series"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "none"
        }
      }
    }
  ]
}